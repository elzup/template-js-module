{
  "name": "TODO",
  "version": "0.0.0",
  "description": "TODO",
  "access": "public",
  "type": "module",
  "license": "MIT",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js",
      "types": "./lib/types/index.d.ts"
    }
  },
  "repository": "elzup/TODO",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json",
    "build:esm": "tsc --module esnext --outDir lib/esm --moduleResolution node --allowSyntheticDefaultImports",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib/types",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "test:badges": "yarn test:cov && istanbul-badges-readme",
    "prepare": "npm run build",
    "depcruise": "depcruise --config .dependency-cruiser.js --include-only \"^src\" --output-type dot src | dot -T svg > dependencygraph.svg",
    "release": "yarn build && yarn publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "oxlint --fix",
      "git add"
    ]
  },
  "files": [
    "lib/cjs",
    "lib/esm",
    "lib/types"
  ],
  "keywords": [
    "utils",
    "personal"
  ],
  "devDependencies": {
    "@elzup/tsconfig": "1.1.0",
    "@types/node": "22.18.1",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "husky": "9.1.7",
    "lint-staged": "13.3.0",
    "oxlint": "^0.15.0",
    "tsd": "0.33.0",
    "typescript": "5.9.2",
    "unimported": "1.31.1",
    "vitest": "^2.1.2"
  },
  "packageManager": "yarn@4.10.3"
}
