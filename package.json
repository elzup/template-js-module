{
  "name": "TODO",
  "version": "0.0.0",
  "description": "TODO",
  "author": "elzup",
  "license": "MIT",
  "type": "module",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    }
  },
  "repository": "elzup/TODO",
  "bugs": {
    "url": "https://github.com/elzup/TODO/issues"
  },
  "homepage": "https://github.com/elzup/TODO#readme",
  "keywords": [
    "utils",
    "personal"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs && echo '{\"type\": \"commonjs\"}' > lib/cjs/package.json",
    "build:esm": "tsc --module esnext --outDir lib/esm --moduleResolution node --allowSyntheticDefaultImports",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib/types",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "files": [
    "lib/"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@elzup/tsconfig": "1.1.0",
    "@types/node": "22.18.1",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "oxlint": "^0.15.0",
    "rimraf": "^5.0.0",
    "typescript": "5.9.2",
    "vitest": "^2.1.2"
  }
}
