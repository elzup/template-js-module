  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "elzup/TODO",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "ts-esm": "node --loader ts-node/esm",
    "lint": "eslint --ext .ts,.tsx",
    "test": "yarn test:jest",
    "test:jest": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:unimported": "unimported",
    "test:cov": "jest src --coverage",
    "test:badges": "yarn test:cov && istanbul-badges-readme",
    "prepare": "npm run build",
    "depcruise": "depcruise --config .dependency-cruiser.js --include-only \"^src\" --output-type dot src | dot -T svg > dependencygraph.svg",
    "release": "yarn build && yarn publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "utils",
    "personal"
  ],
  "devDependencies": {
    "@elzup/tsconfig": "1.1.0",
    "@swc/core": "1.7.26",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.13",
    "@types/node": "20.16.10",
    "babel-jest": "29.7.0",
    "eslint": "8.57.1",
    "eslint-config-nzap": "7.0.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "13.3.0",
    "prettier": "2.8.8",
    "ts-jest": "29.2.5",
    "tsd": "0.31.2",
    "typescript": "5.6.2",
    "unimported": "1.31.1"
  },
  "dependencies": {
    "ts-node": "^10.9.1"
  }
}
