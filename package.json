{
  "name": "TODO",
  "version": "0.0.0",
  "description": "TODO",
  "access": "public",
  "type": "module",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "elzup/TODO",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "ts-esm": "node --loader ts-node/esm",
    "deploy": "npm run build && yarn publish",
    "lint": "eslint --ext .ts,.tsx",
    "test": "yarn test:jest",
    "test:jest": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:unimported": "unimported",
    "test:cov": "jest src --coverage",
    "test:badges": "yarn test:cov && istanbul-badges-readme",
    "prepare": "husky install",
    "depcruise": "depcruise --config .dependency-cruiser.js --include-only \"^src\" --output-type dot src | dot -T svg > dependencygraph.svg",
    "release": "yarn build && yarn publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "git add"
    ]
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "utils",
    "personal"
  ],
  "devDependencies": {
    "@elzup/tsconfig": "1.1.0",
    "@swc/core": "1.2.224",
    "@swc/jest": "0.2.22",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.4",
    "babel-jest": "28.1.3",
    "eslint": "8.21.0",
    "eslint-config-nzap": "6.1.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.7",
    "tsd": "0.22.0",
    "typescript": "4.7.4",
    "unimported": "1.21.0"
  }
}
